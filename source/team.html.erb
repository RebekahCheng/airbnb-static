<div class="text-center" id="team">
  <h1>Our Team</h1>
<ul class="list-inline text-center">
  <% ["anne", "seb", "romain"].each do |member| %>
    <li>
      <h3><%= member.capitalize %></h3>
      <%= image_tag "#{member}.jpg", class: "avatar" %>
    </li>
  <% end %>
</ul>
<h3 class="padded">
  <a href="mailto:contact@airbnb.com">
  <i class="fa fa-envelope"> contact@airbnb.com</i>
  </a>
</h3>
</div>
